[Unit]
Description=SSH Reverse Tunnel Service
After=network.target

[Service]
User=<USERNAME>
ExecStart=/usr/bin/ssh -fN \
          -R YY0:localhost:ZZ0 \
          -R YY1:localhost:ZZ1 \
          -R YY2:localhost:ZZ2 \
          -o ServerAliveInterval=30 \
          -o ServerAliveCountMax=1 \
          <用户名>@<B的IP地址>
TimeoutStopSec=5
KillSignal=SIGTERM
Restart=always
RestartSec=3

[Install]
WantedBy=multi-user.target

